{
    "name": "Diccionario",
    "description": "Diccionario de palabras!",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "128": "128.png"
    },
    "browser_action": {
        "default_title": "Selected Text",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs", "http://*/*", "https://*/*"
    ]
}